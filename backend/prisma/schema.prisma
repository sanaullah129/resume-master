generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGO_URL")
}

model UserDetials {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  username  String   @unique
  emailId   String   @unique
  password  String
  createdAt DateTime @default(now())
}

model ErrorLog {
  id           String   @id @default(auto()) @map("_id") @db.ObjectId
  routeDetails String
  error        String
  errorDate    DateTime @default(now())
}
